  <xs:element name="FindAmpRequest" type="FindAmpRequestType">
    <xs:annotation>
      <xs:documentation>Find an AMP based on one or more criteria</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="FindAmpResponse" type="FindAmpResponseType" />
  <xs:element name="FindCompanyRequest" type="FindCompanyRequestType" />
  <xs:element name="FindCompanyResponse" type="FindCompanyResponseType" />
  <xs:element name="FindVmpRequest" type="FindVmpRequestType" />
  <xs:element name="FindVmpResponse" type="FindVmpResponseType" />
  <xs:element name="FindLegislationTextRequest" type="FindLegislationTextRequestType" />
  <xs:element name="FindLegislationTextResponse" type="FindLegislationTextResponseType" />
  <xs:element name="FindReimbursementRequest" type="FindReimbursementRequestType" />
  <xs:element name="FindReimbursementResponse" type="FindReimbursementResponseType" />
  <xs:element name="FindReferencesRequest" type="FindReferencesRequestType" />
  <xs:element name="FindReferencesResponse" type="FindReferencesResponseType" />
