    </xs:choice>
  </xs:complexType>
  <xs:complexType name="FindByGenericPrescriptionGroupType">
    <xs:choice>
      <xs:element name="GenericPrescriptionGroupCode" type="xs:positiveInteger" />
      <xs:element name="AnyNamePart" type="core:SearchStringType" />
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="FindByCompanyType">
    <xs:choice>
      <xs:element name="CompanyActorNr" type="core:CompanyActorNrType" />
      <xs:element name="AnyNamePart" type="core:SearchStringType" />
