      <xs:element name="Commercialization" type="ConsultCommercializationType" minOccurs="0" />
      <xs:element name="SupplyProblem" type="ConsultSupplyProblemType" minOccurs="0" />
      <xs:element name="Dmpp" type="ConsultDmppType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
