          <xs:element name="Name" type="ConsultTextType" />
        </xs:sequence>
        <xs:attributeGroup ref="validityPeriod" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ConsultVmpComponentType">
    <xs:complexContent>
      <xs:extension base="core:VmpComponentKeyType">
        <xs:sequence>
          <xs:element name="PhaseNumber" type="xs:positiveInteger" minOccurs="0" />
          <xs:element name="Name" type="ConsultTextType" />
