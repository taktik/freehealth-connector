            2 – Generic
            3 – Reference
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Atc" type="AtcClassificationType" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="DeliveryModus" type="DeliveryModusType" />
      <xs:element name="DeliveryModusSpecification" type="DeliveryModusSpecificationType" minOccurs="0" />
      <xs:element name="NoGenericPrescriptionReason" type="NoGenericPrescriptionReasonType" minOccurs="0" maxOccurs="unbounded" />
