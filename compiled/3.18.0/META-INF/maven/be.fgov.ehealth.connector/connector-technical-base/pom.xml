            <artifactId>connector-utilities-ehboxv3</artifactId>
            <version>${version.utilities}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>connector-utilities-timestamping</artifactId>
            <version>${version.utilities}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>${project.groupId}</groupId>
            <artifactId>connector-utilities-etee</artifactId>
            <version>${version.utilities}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
